  object sqlKm: TSQLClientDataSet
    CommandText = 
      'select q.CODIGOVEICULO, m.DESCRICAO, a.SIGLA, v.PLACA, q.DATA, q' +
      '.INICIO, q.FINAL, q.VIAGEM'#13#10'from QUILOMETRAGEM q, VEICULO v, MOD' +
      'ELO m, MARCA a'#13#10'where q.CODIGOVEICULO = v.CODIGOVEICULO'#13#10'  and v' +
      '.CODIGOMODELO = m.CODIGOMODELO'#13#10'  and m.CODIGOMARCA = a.CODIGOMA' +
      'RCA'#13#10'  and q.DATA = :DATA'
